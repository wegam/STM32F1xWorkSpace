void StepMotorConfiguration(SteepMotorDef *STEP_MOTOx)
{
	
	GPIO_Configuration_OPP50	(STEP_MOTOx->SetPulsPort,	STEP_MOTOx->SetPulsPin);			
	
	GPIO_Configuration_OPP50	(STEP_MOTOx->SetDIRPort,	STEP_MOTOx->SetDIRPin);			
	
	TIM_ConfigurationFreq(STEP_MOTOx->SetTIMx,STEP_MOTOx->SetFrequency);					
}








 
unsigned char StepMotorServer(SteepMotorDef *STEP_MOTOx)
{
	if((STEP_MOTOx->SetTIMx->SR & ((u16)0x0001))==((u16)0x0001))			
	{
		if(STEP_MOTOx->GetPulsTotal>=STEP_MOTOx->SetPulsTotal)
		{
			STEP_MOTOx->SetTIMx->CR1 &= ((u16)0x03FE);		

			STEP_MOTOx->SetPulsPort->BRR	=	STEP_MOTOx->SetPulsPin;		
			return 1;
		}
		else if(STEP_MOTOx->PulsFlag	!=	0)
		{
			STEP_MOTOx->PulsFlag	=	0;				
			STEP_MOTOx->SetPulsPort->BRR	=	STEP_MOTOx->SetPulsPin;
			STEP_MOTOx->GetPulsTotal++;				
		}
		else if(STEP_MOTOx->PulsFlag	!=	1)
		{
			STEP_MOTOx->PulsFlag	=	1;				
			STEP_MOTOx->SetPulsPort->BSRR	=	STEP_MOTOx->SetPulsPin;			
		}
		
		
		if(STEP_MOTOx->SetPlusUp	&&	STEP_MOTOx->SetPlusUpNum	&&	(STEP_MOTOx->GetPulsTotal<STEP_MOTOx->SetPlusUpNum)	&&(STEP_MOTOx->PulsFlag	!=0))
		{
			STEP_MOTOx->SetFrequency+=STEP_MOTOx->SetPlusUp;
			TIM_SetFreq(STEP_MOTOx->SetTIMx,STEP_MOTOx->SetFrequency);		
		}
		else if(STEP_MOTOx->SetPlusDown	&&	STEP_MOTOx->SetPlusDownNum	&&	((STEP_MOTOx->GetPulsTotal+STEP_MOTOx->SetPlusDownNum)>STEP_MOTOx->SetPulsTotal)	&&	(STEP_MOTOx->PulsFlag	!=0))
		{
			STEP_MOTOx->SetFrequency-=STEP_MOTOx->SetPlusDown;
			TIM_SetFreq(STEP_MOTOx->SetTIMx,STEP_MOTOx->SetFrequency);		
		}
		STEP_MOTOx->SetTIMx->SR = (u16)~((u16)0x0001);			
		return 1;
	}
	return 0;
}








 
void StepMotorCW(SteepMotorDef *STEP_MOTOx,u16	SetFrequency,u16 SetPlusUp,u16 SetPlusUpNum,u16	SetPlusDown,u16	SetPlusDownNum,u32 SetPulsTotal)		
{
	if(SetFrequency==0	||	SetPulsTotal==0)
	{
		(STEP_MOTOx->SetPulsPort)->BRR	=	STEP_MOTOx->SetPulsPin;
		return;
	}
	else
	{
		(STEP_MOTOx->SetPulsPort)->BRR	=	STEP_MOTOx->SetDIRPin;		
		(STEP_MOTOx->SetPulsPort)->BRR	=	STEP_MOTOx->SetPulsPin;		
		
		STEP_MOTOx->SetFrequency		=	SetFrequency;							
		STEP_MOTOx->SetPlusUp				=	SetPlusUp;								
		STEP_MOTOx->SetPlusUpNum		=	SetPlusUpNum;							
		
		STEP_MOTOx->SetPlusDown			=	SetPlusDown;							
		STEP_MOTOx->SetPlusDownNum	=	SetPlusDownNum;						
		
		STEP_MOTOx->SetPulsTotal		=	SetPulsTotal;							
		
		
		STEP_MOTOx->RunFlag					=	0;												
		STEP_MOTOx->PulsFlag				=	0;												
		
		STEP_MOTOx->GetFrequency		=	SetFrequency;							
		STEP_MOTOx->GetPlusUpNum		=	0;												
		STEP_MOTOx->GetPulsTotal		=	0;												

		TIM_SetFreq(STEP_MOTOx->SetTIMx,STEP_MOTOx->SetFrequency);		
		
		STEP_MOTOx->SetTIMx->CR1 |= ((u16)0x0001);							


	}
}








 
void StepMotorCCW(SteepMotorDef *STEP_MOTOx,u16	SetFrequency,u16 SetPlusUp,u16 SetPlusUpNum,u16	SetPlusDown,u16	SetPlusDownNum,u32 SetPulsTotal)		
{
	if(SetFrequency==0	||	SetPulsTotal==0)
