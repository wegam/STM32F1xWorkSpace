		STEP_MOTOx->GetFrequency		=	SetFrequency;							
		STEP_MOTOx->GetPlusUpNum		=	0;												
		STEP_MOTOx->GetPulsTotal		=	0;												
		
		TIM_SetFreq(STEP_MOTOx->SetTIMx,STEP_MOTOx->SetFrequency);		
		
		STEP_MOTOx->SetTIMx->CR1 |= ((u16)0x0001);							

	}
}








 
void StepMotorStop(SteepMotorDef *STEP_MOTOx)
{
	STEP_MOTOx->SetTIMx->CR1 &= ((u16)0x03FE);		
	STEP_MOTOx->SetPulsPort->BRR	=	STEP_MOTOx->SetPulsPin;		
	
	STEP_MOTOx->SetFrequency		=	0;						
	STEP_MOTOx->SetPlusUp				=	0;						
	STEP_MOTOx->SetPlusUpNum		=	0;						
	
	STEP_MOTOx->SetPlusDown			=	0;						
	STEP_MOTOx->SetPlusDownNum	=	0;						
	
	STEP_MOTOx->SetPulsTotal		=	0;						
	
	STEP_MOTOx->RunFlag					=	0;						
	STEP_MOTOx->PulsFlag				=	0;						
	
	STEP_MOTOx->GetFrequency		=	0;						
	STEP_MOTOx->GetPlusUpNum		=	0;						
	STEP_MOTOx->GetPulsTotal		=	0;						
}








 
